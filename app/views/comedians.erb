<body>
  <h2>Average age = <%= (Comedian.all.map(&:age).sum)/(Comedian.all.count)%>
  <h3>Name, Age, Specials</h3>
  <% @comedians.each do |comedian| %>
    <%= comedian.name %>
    <%= comedian.age %>
    <p>Number of specials: <%= comedian.specials.count %></p>
    <% comedian_specials = comedian.specials.map(&:name)%>
    <%= comedian_specials %><br/>
  <% end %>
</body>
